#后缀名屏蔽显示Forbidden
<FilesMatch "\.(bak|inc|lib|sh|tpl|lbi|dwt)$">
    order deny,allow
    deny from all
</FilesMatch>
Options -Indexes
RewriteEngine On
#rewriteBase /member

#RewriteBase /
#RewriteRule
#屏蔽非php扩展的动态文件，例如asp、aspx等，可以阻止asp、aspx等后缀的后门程序运行
RewriteRule ^(.*)\.(asp|aspx|asa|asax|dll|jsp|cgi|fcgi|pl)(.*)$ /404.php
#屏蔽站点file目录php运行权限，站点的file目录默认具有写入权限，当网站出现未知漏洞时，可能会被写入后门程序，阻止php运行之后，即使有后门程序也将无法运行。
RewriteRule ^(.*)/data/(.*)\.php(.*)$ /404.php
#投资产品规则
#RewriteRule ^([a-zA-Z]+)-([0-9]+)\.html$ index.php?category=$1&id=$2

#借款规则

